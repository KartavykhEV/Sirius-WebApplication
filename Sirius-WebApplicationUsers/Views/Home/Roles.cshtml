@using Microsoft.AspNetCore.Identity
@{
    ViewData["Title"] = "Роли";
}

@model IEnumerable<IdentityRole>
<h1>@ViewData["Title"]</h1>

@if (Model.Count() > 0)
{
    <ul>
        @foreach (var role in Model)
        {
            <li>@role.Name</li>
        }
    </ul>
}
else
{
    <p>Ролей еще нет</p>
}
